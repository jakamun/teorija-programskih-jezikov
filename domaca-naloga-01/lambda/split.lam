LET reverse = REC reverse acc -> FUN sez ->
    MATCH sez WITH
    | [] -> acc
    | x :: xs -> reverse (x :: acc) xs 
IN
LET split1 = REC split1 acc1 -> FUN acc2 -> FUN sez ->
    MATCH sez WITH
    | [] -> {reverse [] acc1, reverse [] acc2}
    | x :: xs -> split1 ((FST x) :: acc1) ((SND x) :: acc2) xs
IN
LET REC split sez = split1 [] [] sez
IN
split ({7, 3} :: {4, 2} :: {5,6} :: [])